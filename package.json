{
  "name": "expressjs-backend-template",
  "version": "1.0.0",
  "description": "A secure Express.js backend template",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest \"src/**/__tests__/**/*.test.ts\"",
    "test:integration": "jest \"tests/integration/**/*.test.ts\"",
    "test:e2e": "jest \"tests/e2e/**/*.test.ts\"",
    "test:all": "jest --runInBand",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@redis/client": "^5.1.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/ioredis": "^4.28.10",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "redis": "^5.1.1",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^22.15.29",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
